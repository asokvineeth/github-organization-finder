<div class="modal {{if showModal 'showModal' ''}}">
  <label class="modal-outer" for="modal-2"></label>
  <div class="modal-inner">
    <label class="modal-close" for="modal-2" {{action 'toggleData' 'showModal'}}></label>
    <h2>Branch Details</h2>
    <p> Number of branches: {{branches.length}}</p>
    <h2>List of Branches</h2>
    {{#each branches as |branch|}}
      <p>{{branch.name}}</p>
    {{/each}}
  </div>
</div>
